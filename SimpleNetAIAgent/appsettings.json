{
  "ApiDetails": {
    "BaseUrl": "https://api.example.com" //change as per your requirement
  },
  "EvaluatorDetails": {
    "Model": "your model" //change as per your requirement
  },
  "LlmDetails": {
    "ApiKey": "your key", //change as per your requirement
    "Endpoint": "https://api.openai.com/v1/chat/completions", //change as per your requirement
    "MaxOutputTokens": 1024, //change as per your requirement
    "Model": "your model", //change as per your requirement
    "TelemetryEndpoint": "https://api.openai.com/v1/telemetry", //change as per your requirement
    "Temperature": 0 //change as per your requirement
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "NerDetails": {
    "Model": "your model" //change as per your requirement
  },
  "RagDetails": {
    "Location": "./RagData", //change as per your requirement
    "MaxGoodFilesPerField": 3, //change as per your requirement
    "MaxRetrivedModels": 5, //change as per your requirement
    "MaxWrongFilesPerField": 8 //change as per your requirement
  },
  "Serilog": {
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "SimpleNETAIAgent" //change as per your requirement
    },
    "WriteTo": [
      {
        "Args": {
          "formatter": {
            "template": "{ {TS: ToString(@t, 'MM/dd/yy hh:mm:ss.ffffff tt'), MSG: @m, LVL: @l, EX: @x, ..@p} }\n",
            "type": "Serilog.Expressions.ExpressionTemplate, Serilog.Expressions"
          }
        },
        "Name": "Console"
      }
    ]
  }
}